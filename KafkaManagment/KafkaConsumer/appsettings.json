{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KafkaConfiguration": {
    "ConsumerBootstrapServers": "KafkaAddress",
    "GroupId": "sendMessage",
    "EnableAutoOffsetStore": true,
    "EnableAutoCommit": true,
    "AutoOffsetReset": 0,
    "ConsumerTopicName": "producerTopicName",
    "ProducerTopicName": ""
  }
}
